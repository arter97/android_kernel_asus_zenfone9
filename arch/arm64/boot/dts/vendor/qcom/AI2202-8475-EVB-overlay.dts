/ {
	model = "Qualcomm Technologies, Inc. ASUS AI2202 8475 EVB ER1";
	compatible = "qcom,cape-mtp", "qcom,cape", "qcom,mtp";
	qcom,msm-id = <530 0x10000>, <531 0x10000>, <540 0x10000>;
	qcom,board-id = <36 0>;
};
&aliases {
	i2c0 = "/soc/i2c@990000";
};

